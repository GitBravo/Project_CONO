<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.kumoh.s20130053.cono">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SignInActivity"
            android:theme="@style/AppTheme.Signin_Theme">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/hairshop_select"
            android:theme="@style/AppTheme.MainTheme" />
        <activity
            android:name=".TabActivity"
            android:label="@string/title_activity_tab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NaviLogActivity" />
        <activity android:name=".NaviResActivity" />
        <activity android:name=".NaviMyResActivity" />
        <activity android:name=".NaviModifyActivity" />
        <activity android:name=".CounselingDetailActivity" />
        <activity android:name=".CounselingWriteActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.Signin_Theme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>